{
  "name": "atuin",
  "version": "1.0.0",
  "description": "A Turtle RDF editor with graph visualization",
  "type": "module",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack serve --mode development",
    "test": "jasmine --config=jasmine.json",
    "docs": "jsdoc -c jsdoc.json",
    "ghp": "webpack --mode production && docs/github-pages/deploy.sh",
    "rp": "repomix -c repomix.config.json ."
  },
  "keywords": [
    "rdf",
    "turtle",
    "editor",
    "graph",
    "visualization"
  ],
  "author": "Danny Ayers",
  "license": "MIT",
  "dependencies": {
    "@codemirror/language": "^6.9.3",
    "@codemirror/state": "^6.4.0",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@codemirror/view": "^6.24.0",
    "@lezer/highlight": "^1.2.0",
    "@rdfjs/data-model": "^2.0.1",
    "@rdfjs/namespace": "^2.0.0",
    "@rdfjs/parser-n3": "^2.0.1",
    "@rdfjs/serializer-ntriples": "^2.0.1",
    "n3": "^1.17.2",
    "rdf-ext": "^2.2.0",
    "readable-stream": "^4.4.2",
    "string-to-stream": "^3.0.1",
    "vis-data": "^7.1.9",
    "vis-network": "^9.1.6"
  },
  "devDependencies": {
    "chai": "^4.5.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^6.10.0",
    "docdash": "^2.0.2",
    "html-webpack-plugin": "^5.6.0",
    "jasmine": "^5.1.0",
    "jasmine-browser-runner": "^2.3.0",
    "jasmine-core": "^5.1.0",
    "jsdoc": "^4.0.2",
    "jsdom": "^26.1.0",
    "style-loader": "^3.3.4",
    "vitest": "^3.1.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
